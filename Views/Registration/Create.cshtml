      //======================================================set value====================================================...
            function Setvalue() {
                var _lNo=""
                _lNo = document.getElementById("Membership_Number").value;
                var MyAppUrl = { Url: '@Url.Action("FatchDoctorsDetails", "Registration")' }
                     $.ajax({
                         url: MyAppUrl.Url,
                         data: { lno: _lNo },
                         cache: false,
                         type: "POST",
                         success: function (data) {
                             if (data[0]['fname'] == null) {
                                 alert(data[0]['fname']);
                             }
                             else {
                                 //confirm(data);
                                 document.getElementById("register_fullname").value = data[0]['fname'] + " " + data[0]['lname'];
                                 document.getElementById("txt_Address").value = data[0]['address'];
                                 document.getElementById("txt_Pin").value = data[0]['pin'];
                                 document.getElementById("txt_City").value = data[0]['city'];
                                 document.getElementById("txt_Email").value = data[0]['email'];
                                 document.getElementById("example1").value = data[0]['dob'];
                                 document.getElementById("txt_Fax").value = data[0]['fax'];
                                 document.getElementById("txt_Work_Phone").value = data[0]['phone'];
                                 document.getElementById("txt_Designation").value = data[0]['designation'];
                                 //Category dropdown
                                 $("#ddlcsiCategory").empty();
                                 var o = document.createElement("option");
                                 o.value = "1"; o.text = "CSI Member";
                                 $("#ddlcsiCategory").append(o);

                                 //state dropdown
                                 var p = document.createElement("option");
                                 $("#register_state").empty();
                                 p.value = data[0]['state'];
                                 p.text = data[0]['state'];
                                 $("#register_state").append(p);


                             }
                         },
                         error: function (reponse) {
                             confirm(data);
                             //alert("Permission denied.");
                             location.reload(true);
                         }
                     });
                 
                return false;
            }
